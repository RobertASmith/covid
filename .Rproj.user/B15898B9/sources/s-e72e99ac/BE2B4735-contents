    title = {EpiEstim: Estimate Time Varying Reproduction Numbers from Epidemic Curves},
    author = {Anne Cori},
    year = {2019},
    note = {R package version 2.2-1},
    url = {https://CRAN.R-project.org/package=EpiEstim},
